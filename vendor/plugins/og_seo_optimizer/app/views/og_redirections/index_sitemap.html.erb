
<h2><%= t('og_redirections.sitemap_manage') %></h2>

<%= render :partial => "menu" %>

<div class="esq-top-izq">
  <div class="esq-top-der">
    <div class="esq-bottom-der">
      <div class="esq-bottom-izq">
        <div class="caja-formulario">

          <% form_tag({:action => 'index_sitemap'},:multipart => true  )do %>


            <div>
              <label for="sitemap">sitemap.xml(.gz)</label>
              <%= file_field_tag "sitemap"%>
            </div>
            <div class="content-botones-cms">
              <div class="botones-cms guardar">
                <%= submit_tag t('main.submit'), :class => 'boton' %>
              </div>
            </div>
          <% end %>

        </div>
      </div>
    </div>
  </div>
</div>